<!doctype html>
<html lang="es">
	<head>
		<title>Events info</title>
		<meta charset="utf-8">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js"></script>
		<link rel="stylesheet" type="text/css" href="../expertises.css">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
	</head>
	<body>
		<div class="row">
			<div class="col-md-1"></div>
				<div class="col-md-3" id="header">
					<a href="http://localhost:3000/"><li id="site_name">42API HOME</li></a>
				</div>
				<div class="col-md-1"></div>
				<div class="col-md-2" id="header">
					<a href="http://localhost:3000/static/personal/me"><li id="site_name">Personal</li></a>
				</div>
				<div class="col-md-4" id="header">
					<a href="http://localhost:3000/static/expertises/66024"><li id="site_name">Expertises</li></a>
				</div>
				<div class="col-md-1"></div>
		</div>
	<br>
	<div class="row">
		<div class="col-md-8"></div>
		<div class="col-md-2">
			<nav aria-label="Page navigation example">
				<ul class="pagination justify-content-end">
					<li class="page-item active">
							<a class="page-link" href="http://localhost:3000/static/next" title="Go to next page">Next</a>
					</li>
				</ul>
			</nav>
		</div>
		<div class="col-md-2"></div>
	</div>
	<div class="row">
		<div class="col-md-1"></div>
		<div class="col-md-5">
			<% if (expertises.length > 0) { %>
			<h4>Expertises from <%= expertises[0].user.login %></h4>
				<table class="table table-hover table-dark">
					<tr>
						<th>Name</th>
						<th>Level</th>
						<th>ID</th>
						<th>Expertise ID</th>
						<th>Interested</th>
						<th>Contact</th>
					</tr>
					<% for (var x = 0; x < expertises.length; x++) { %>
						<tr>
							<td> <%= expertises[x].expertise.name %> </td>
							<td> <%= expertises[x].value %> </td>
							<td> <%= expertises[x].id %> </td>
							<td> <%= expertises[x].expertise_id %> </td>
							<td> <%= expertises[x].interested %> </td>
							<td> <%= expertises[x].contact_me %> </td>
						</tr>
						<% } %>
				</table>
				<% } else { %>
					<h4>This login has not expertises or not exist</h4>
				<% } %>
		</div>
		<div class="col-md-3"></div>
		<div class="col-md-3"></div>
	</div>
	</body>
</html>
